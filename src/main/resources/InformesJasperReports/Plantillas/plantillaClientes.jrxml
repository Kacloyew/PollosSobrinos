<jasperReport
        xmlns="http://jasperreports.sourceforge.net/jasperreports"
        name="ListadoClientes" pageWidth="900" pageHeight="750" orientation="Landscape">

    <parameter name="Titulo" class="java.lang.String"/>
    <parameter name="Autor" class="java.lang.String"/>
    <parameter name="Fecha" class="java.lang.String"/>

    <queryString>
        <![CDATA[SELECT Cliente_ID, Nombre, Apellido, NIF_NIE, Telefono, CorreoElectronico, Tienda_ID
             FROM Clientes
             ORDER BY Cliente_ID]]>
    </queryString>

    <field name="Cliente_ID" class="java.lang.Integer"/>
    <field name="Nombre" class="java.lang.String"/>
    <field name="Apellido" class="java.lang.String"/>
    <field name="NIF_NIE" class="java.lang.String"/>
    <field name="Telefono" class="java.lang.String"/>
    <field name="CorreoElectronico" class="java.lang.String"/>
    <field name="Tienda_ID" class="java.lang.Integer"/>

    <title>
        <band height="80">
            <textField>
                <reportElement x="20" y="10" width="800" height="40"/>
                <textElement textAlignment="Center">
                    <font size="24" isBold="true"/>
                </textElement>
                <textFieldExpression><![CDATA[$P{Titulo}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="20" y="50" width="800" height="20" />
                <textElement textAlignment="Center"/>
                <textFieldExpression><![CDATA["Realizado por: " + $P{Autor} + " - Fecha: " + $P{Fecha}]]></textFieldExpression>
            </textField>
        </band>
    </title>

    <columnHeader>
        <band height="30">
            <rectangle>
                <reportElement x="20" y="0" width="800" height="25" />
                <graphicElement>
                    <pen lineWidth="1.0"/>
                </graphicElement>
            </rectangle>
            <staticText>
                <reportElement x="25" y="5" width="100" height="15" />
                <textElement>
                    <font isBold="true"/>
                </textElement>
                <text><![CDATA[CLIENTE-ID]]></text>
            </staticText>
            <staticText>
                <reportElement x="125" y="5" width="150" height="15" />
                <textElement>
                    <font isBold="true"/>
                </textElement>
                <text><![CDATA[NOMBRE]]></text>
            </staticText>
            <staticText>
                <reportElement x="275" y="5" width="150" height="15" />
                <textElement>
                    <font isBold="true"/>
                </textElement>
                <text><![CDATA[APELLIDO]]></text>
            </staticText>
            <staticText>
                <reportElement x="425" y="5" width="100" height="15" />
                <textElement>
                    <font isBold="true"/>
                </textElement>
                <text><![CDATA[NIF/NIE]]></text>
            </staticText>
            <staticText>
                <reportElement x="525" y="5" width="100" height="15" />
                <textElement>
                    <font isBold="true"/>
                </textElement>
                <text><![CDATA[TELÃ‰FONO]]></text>
            </staticText>
            <staticText>
                <reportElement x="625" y="5" width="150" height="15" />
                <textElement>
                    <font isBold="true"/>
                </textElement>
                <text><![CDATA[EMAIL]]></text>
            </staticText>
            <staticText>
                <reportElement x="775" y="5" width="50" height="15" />
                <textElement>
                    <font isBold="true"/>
                </textElement>
                <text><![CDATA[TIENDA]]></text>
            </staticText>
        </band>
    </columnHeader>

    <detail>
        <band height="25">
            <textField>
                <reportElement x="25" y="5" width="100" height="15" />
                <textFieldExpression><![CDATA[$F{Cliente_ID}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="125" y="5" width="150" height="15" />
                <textFieldExpression><![CDATA[$F{Nombre}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="275" y="5" width="150" height="15" />
                <textFieldExpression><![CDATA[$F{Apellido}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="425" y="5" width="100" height="15" />
                <textFieldExpression><![CDATA[$F{NIF_NIE}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="525" y="5" width="100" height="15" />
                <textFieldExpression><![CDATA[$F{Telefono}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="625" y="5" width="150" height="15" />
                <textFieldExpression><![CDATA[$F{CorreoElectronico}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="775" y="5" width="50" height="15" />
                <textFieldExpression><![CDATA[$F{Tienda_ID}]]></textFieldExpression>
            </textField>
        </band>
    </detail>

    <summary>
        <band height="40">
            <rectangle>
                <reportElement x="20" y="0" width="800" height="25" />
                <graphicElement>
                    <pen lineWidth="1.0"/>
                </graphicElement>
            </rectangle>
            <textField>
                <reportElement x="30" y="5" width="300" height="15" />
                <textElement textAlignment="Left">
                    <font isBold="true"/>
                </textElement>
                <textFieldExpression><![CDATA["Total Registros: " + $V{REPORT_COUNT}]]></textFieldExpression>
            </textField>
        </band>
    </summary>
</jasperReport>